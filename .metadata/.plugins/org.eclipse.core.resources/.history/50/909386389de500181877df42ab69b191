#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define False 0
#define True !False
#define OMEGA ('Z' - 'A')

int main(void) {
	int done;
	long int r;
	char alpha;
	srandom((unsigned)time(NULL));
	done=False;
	while(!done){
		r = random() % OMEGA;
		alpha = 'A' + (char)r;
		if(alpha=='Q') done=True;
		putchar(alpha);
	}
	putchar('\n');
	return EXIT_SUCCESS;
}
