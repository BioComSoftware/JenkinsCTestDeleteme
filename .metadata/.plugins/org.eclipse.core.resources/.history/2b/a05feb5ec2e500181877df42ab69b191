#include <time.h>
#include <unistd.h>

#define False 0
#define True !False

char testfunc(int i);

int main(void) {
	char j = 'A';
	j = testfunc(j);
	printf("%c", j);
	return EXIT_SUCCESS;
}

char testfunc(char i){ return i+1; }

