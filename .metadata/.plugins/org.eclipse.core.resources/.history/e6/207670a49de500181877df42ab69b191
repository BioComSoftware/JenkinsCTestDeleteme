#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define False 0
#define True !False
#define OMEGA ('Z' - 'A')

int main(void) {
	int done;
	long int r;
	char alpha;
	done=False;
	int count = 0;
	for(int i=0; i<10; i++) {
		while(!done){
			srandom((unsigned)time(NULL));
			r = random() % OMEGA;
			alpha = 'A' + (char)r;
			if(alpha=='Q' && count > 8) done=True;
			putchar(alpha);
			count += 1;
		}
	putchar('\n');
	}
	return EXIT_SUCCESS;
}
